{
  "name": "list-functions",
  "displayName": "List Functions",
  "description": "A VSCode extension to list functions in JS/TS files",
  "version": "0.0.1",
  "publisher": "kacoulib",
  "repository": {
    "type": "git",
    "url": "https://github.com/kacoulib/vscode-file-struct.git"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.listFunctionsFromExplorer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.listFunctionsFromExplorer",
        "title": "List Functions from Explorer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.listFunctionsFromExplorer",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^10.0.0",
    "@types/mocha": "^2.2.42",
    "typescript": "^3.9.10",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.0",
    "@types/vscode": "1.51.0"
  }
}
